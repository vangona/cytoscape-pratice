    "outDir": "./dist/",
    "noImplicitAny": true,
    "module": "es6",
    "target": "es5",
    "allowSyntheticDefaultImports": true, // cytoscape 호환
    "esModuleInterop": true, // cytoscape 호환
    "allowJs": true,
    "moduleResolution": "node"
  },
  "include": ["src/**.ts"]
